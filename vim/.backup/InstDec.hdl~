CHIP InstDec {
    IN in[16];
    OUT a,zx,nx,zy,ny,f,no,writeMem,writeD,writeA,jgtz,jeqz,jltz;

    PARTS:
    And(a=in[0],b=true,out=jgtz);
    And(a=in[1],b=true,out=jeqz);
    And(a=in[2],b=true,out=jltz);
    And(a=in[3],b=true,out=writeMem);
    And(a=in[4],b=true,out=writeD);
    And(a=in[5],b=true,out=writeA);
    And(a=in[6],b=true,out=zx);
    And(a=in[7],b=true,out=nx);
    And(a=in[8],b=true,out=zy);
    And(a=in[9],b=true,out=ny);
    And(a=in[10],b=true,out=f);
    And(a=in[11],b=true,out=no);
    And(a=in[12],b=true,out=a);
}

