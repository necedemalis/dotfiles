// File name: projects/04/Mult.asm

// Multiplies R0 and R1 and stores the result in R2.
// (R0, R1, R2 refer to RAM[0], RAM[1], and RAM[3], respectively.)

// Put your code here.
@i
M=1
@sum
M=0
(Loop)
        @i
        D=M
        @R1
        D=M-D
        @Loop2
        D;JLT
        @R0
        D=M
        @sum
        M=M+D
        @i
        M=M+1
        @Loop
        0;JMP
(Loop2)
        @sum
        D=M
        @R3
        M=D
        @END
        0;JMP
(END)
        @END
        0;JMP


